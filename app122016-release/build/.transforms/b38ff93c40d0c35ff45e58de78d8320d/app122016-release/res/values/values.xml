    <declare-styleable name="FormItemEditText"><attr format="enum" name="pinAnimationType"><enum name="popIn" value="0"/><enum name="fromBottom" value="1"/><enum name="none" value="-1"/></attr><attr format="string" name="pinCharacterMask"/><attr format="dimension" name="pinCharacterSize"/><attr format="dimension" name="pinFontSize"/><attr format="string" name="pinRepeatedHint"/><attr format="dimension" name="pinLineStroke"/><attr format="dimension" name="pinLineStrokeSelected"/><attr format="boolean" name="pinLineStrokeCentered"/><attr format="dimension" name="pinCharacterSpacing"/><attr format="dimension" name="pinTextBottomPadding"/><attr format="color" name="pinLineColors"/><attr format="reference" name="pinBackgroundDrawable"/><attr format="boolean" name="pinBackgroundIsSquare"/></declare-styleable>
    <declare-styleable name="FormItemView"><attr format="string" name="formTitle"/><attr format="integer" name="formInputLength"/><attr format="string" name="formValidationError"/><attr format="boolean" name="formActionOnTop"/></declare-styleable>
    <declare-styleable name="Keypad"><attr format="color" name="keypad_bg_color"/><attr format="color" name="key_digit_color"/><attr format="dimension" name="key_digit_size"/><attr format="dimension" name="key_digit_height"/></declare-styleable>
    <dimen name="activity_horizontal_margin">10dp</dimen>
    <dimen name="activity_vertical_margin">5dp</dimen>
    <dimen name="button_padding">5dp</dimen>
    <dimen name="keyboard_height">264dp</dimen>
    <string name="action_hide">HIDE</string>
    <string name="action_resend">RESEND</string>
    <string name="action_show">SHOW</string>
    <string name="app_name">NPCI Secure Payment</string>
